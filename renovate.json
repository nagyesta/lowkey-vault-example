{
  "extends": [
    "config:recommended",
    ":gitSignOff",
    "group:allNonMajor",
    "schedule:earlyMondays"
  ],
  "assignees": [
    "nagyesta"
  ],
  "assigneesSampleSize": 1,
  "commitMessageSuffix": "{patch}",
  "github-actions": {
    "pinDigests": true
  },
  "customManagers": [
    {
      "name": "testcontainers-docker",
      "customType": "regex",
      "managerFilePatterns": [
        "/\\.java$/"
      ],
      "matchStrings": [
        "DockerImageName\\.parse\\(\\s*\"(?<depName>[^:]+):(?<currentValue>[^\"']+)\"\\s*\\)"
      ],
      "datasourceTemplate": "docker",
      "versioningTemplate": "docker"
    }
  ]
}
